"""def palo(n):
    if n == int(str(n)[::-1]):
        return True
    else:
        return False

x = int(input("3 veya 4 basamakli bir sayi giriniz : "))
if x<100 or x>1000:
    print("3 VEYA 4 BASAMAKLI GIR!")
